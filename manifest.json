{
  "name": "Feedly Background Tab Open",
  "description": "Open Feedly Links in Background Tab using shortcut key",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.feedly.com/*"],
      "run_at": "document_idle",
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
